package sample;

import java.util.Scanner;

/**
 Выведите на экран первые 20 членов последовательности Фибоначчи.
 */
public class SequenceFibonachi {
    public static void main(String[] args) {
        int a = 1;
        int b = 1;
        int n = 20;
        int sum_fib;
        for (i = 0; i <= n; i++) {
            sum_fib = a + b;
            a = b;
            b = sum_fib;
            System.out.println(sum_fib + " ");
        }
    }
